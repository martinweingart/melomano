{
  "name": "melomano",
  "version": "0.1.0",
  "description": "Local music server",
  "main": "electron/index.js",
  "scripts": {
    "start": "set DEV_MODE=true && electron ./electron/index.js",
    "serve": "nodemon --ignore ./webapp ./scripts/serve.js",
    "scan": "node ./scripts/scan.js",
    "build": "node ./scripts/build.js"
  },
  "keywords": [
    "music",
    "stream"
  ],
  "author": "Martin Weingart",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-zip": "^3.0.0",
    "joi": "^17.6.0",
    "lokijs": "^1.5.12",
    "mime-types": "^2.1.35",
    "music-metadata": "^7.12.4",
    "pino": "^8.4.0"
  },
  "devDependencies": {
    "electron": "^20.0.1",
    "fs-extra": "^10.1.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1"
  }
}
